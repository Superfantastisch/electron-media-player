{
  "name": "electron-media-player",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && electron .",
    "electron-aot": "ng build --prod --base-href ./ && electron .",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:build": "tsc -p tsconfig-serve.json && ng build --base-href ./ && electron-packager . --all --arch=x64",
    "electron:build-mac": "electron-packager . --platform=darwin --arch=x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
    "@angular/cdk": "^7.0.4",
    "@angular/common": "~7.0.4",
    "@angular/compiler": "~7.0.4",
    "@angular/core": "~7.0.4",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "~7.0.4",
    "@angular/http": "~7.0.4",
    "@angular/material": "^7.0.4",
    "@angular/platform-browser": "~7.0.4",
    "@angular/platform-browser-dynamic": "~7.0.4",
    "@angular/router": "~7.0.4",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "shaka-player": "^2.5.0-beta2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.4",
    "@angular/language-service": "~7.0.4",
    "@types/jasmine": "~3.0.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.9",
    "codelyzer": "~4.5.0",
    "electron": "^3.0.9",
    "electron-packager": "^13.0.1",
    "electron-reload": "^1.3.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "wait-on": "^3.2.0"
  }
}
